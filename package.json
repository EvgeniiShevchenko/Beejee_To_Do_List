{
  "name": "beeje_project",
  "version": "1.0.0",
  "description": "MERN(project)",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "index": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run index\" \"npm run client\"",
    "build": "babel src --out-file index.js  --presets es2015",
    "watch": "babel src --out-dir ./  -w",
    "launch": "concurrently \"npm run watch\" \"npm run index\"",
    "sinc": "concurrently \"npm run watch\" \"npm run index\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1"
  },
  "author": "Evgenii Shevchenko",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "bson": "^4.0.2",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mongodb-stitch-core-sdk": "^4.4.0",
    "mongodb-stitch-server-sdk": "^4.4.0",
    "mongoose": "^5.6.2",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-stage-2": "^7.0.0",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.1"
  }
}
